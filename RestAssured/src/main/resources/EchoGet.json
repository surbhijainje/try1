{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "args",
        "headers",
        "url"
    ],
    "properties": {
        "args": {
            "type": "object",
            "default": {},
            "title": "The args Schema",
            "required": [],
            "properties": {},
            "examples": [{}]
        },
        "headers": {
            "type": "object",
            "default": {},
            "title": "The headers Schema",
            "required": [
                "x-forwarded-proto",
                "x-forwarded-port",
                "host",
                "x-amzn-trace-id",
                "accept",
                "user-agent",
                "accept-encoding"
            ],
            "properties": {
                "x-forwarded-proto": {
                    "type": "string",
                    "default": "",
                    "title": "The x-forwarded-proto Schema",
                    "examples": [
                        "https"
                    ]
                },
                "x-forwarded-port": {
                    "type": "string",
                    "default": "",
                    "title": "The x-forwarded-port Schema",
                    "examples": [
                        "443"
                    ]
                },
                "host": {
                    "type": "string",
                    "default": "",
                    "title": "The host Schema",
                    "examples": [
                        "postman-echo.com"
                    ]
                },
                "x-amzn-trace-id": {
                    "type": "string",
                    "default": "",
                    "title": "The x-amzn-trace-id Schema"
                    
                },
                "accept": {
                    "type": "string",
                    "default": "",
                    "title": "The accept Schema"
                    
                },
                "user-agent": {
                    "type": "string",
                    "default": "",
                    "title": "The user-agent Schema"
                    
                },
                "accept-encoding": {
                    "type": "string",
                    "default": "",
                    "title": "The accept-encoding Schema",
                    "examples": [
                        "gzip,deflate"
                    ]
                }
            }
            
        },
        "url": {
            "type": "string",
            "default": "",
            "title": "The url Schema",
            "examples": [
                "https://postman-echo.com/get"
            ]
        }
    }
    
}